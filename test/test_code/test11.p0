var int i, int[100] a, int[100] b;
begin
    i := 0;
    while i < 100 do
    begin
        b[i] := i;
        i := i + 1
    end;
    i := 0;
    while i < 100 do
    begin
        a[i] := b[i];
        i := i + 1
    end;
    i := 0;
    while i < 100 do
    begin
        !a[i];
        i := i + 1
    end;
end.